<main>
    <div>
        <ul>
            <div class="card">
                <div class="card-body">
                    {{#with heading}}
                    <div class="card">
                        <div class="card-body">
                            {{#with main}}
                            <h1>{{title}}</h1>
                            <p>{{text}}</p>
                           
                            {{#with button_text}}
                            <div>
                                <button class="btn btn-primary mb-2">{{solid}}</button>
                            </div>
                            <div>
                                <button class="btn btn-outline-primary col-4">{{outline}}</button>
                            </div>
                            {{/with}}
                            
                        {{/with}}
                        </div>
                    </div>
                        <br>
                    <div class="card">
                        <div class="card-body">    
                            {{#with second}}
                                <h2>{{title}}</h2>
                                <p>{{text}}</p>
                                {{#with button_text}}
                                <div>
                                    <button class="btn btn-secondary mb-2">{{solid}}</button>
                                </div>
                                <div>
                                    <button class="btn btn-outline-secondary">{{outline}}</button>
                                </div> 
                                {{/with}}
                            {{/with}}
                        </div>
                    </div>                
                        <br>
                    <div class="card">
                        <div class="card-body">
                            {{#with subblock}}
                                <h3>{{title}}</h3>
                                <p>{{text}}</p>
                                {{#with button_text}}
                                <div>
                                    <button class="btn btn-dark mb-2 col-2">{{solid}}</button>
                                </div>
                                <div>
                                     <button class="btn btn-outline-dark col-2 ">{{outline}}</button>
                                </div>   
                                {{/with}}
                            {{/with}}
                        </div>
                    </div>  
                    {{/with}}
                </div>
            </div>  
            <br>
            <br>
            <div class="card">
                <div class="card-body">
                    {{#with nested_display}}
                    
                        <div class="display-1">{{title}}</div>
                            <div class="row">
                                <div class="col-8">
                                    <div class="card">
                                        <div class="card-body">
                                        {{#with children_display}}
                                            <div class="display-4">{{title}}</div>
                                            <p>{{text}}</p>
                                            {{#with button}}
                                                <button class="btn btn-outline-warning">{{text}}</button>
                                            {{/with}}
                                        {{/with}}
                                    </div>
                                </div>
                            </div>
                        </div>
                       
                        <br>
                        <br>
                
                        <h6>{{header_text}}</h6>
                            {{#with button}}
                                <button class="btn btn-info">{{text}}</button>
                            {{/with}}
                    {{/with}}
                </div>
            </div>                    
            <br>
            <br>
                <div class="card">
                    <div class="card-body">
                        {{#with paragraph}}
                            <p class="display-6">{{title}}</p>
                                <p>{{text}}</p>
                            {{#with button}}
                                
                                <a href="{{url}}" class="btn btn-link" >Link</a>
                            {{/with}}
                            <br>
                            <br>
                            <div class="card">
                                <div class="card-body">
                                    {{#each list_name}}
                                        <li><a href="{{url}}" class="btn btn-link" >Link</a></li>
                                    {{/each}}
                                </div>
                            </div>    
                        {{/with}} 
                    </div>
                </div>

        </ul>
    </div>
</main>

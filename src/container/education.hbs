
<div>
   {{#with main}}
        <main>
            <section>
                 <h2><strong>My educations1</strong></h2> 
                <ul>
                    {{#each educations}}
                    <li>
                        {{name}} 
                        {{#unless isEnd isEnd = false}} '(ще в процесі)' {{/unless}}
                        
                    </li>
                   {{/each}}
                </ul>
            </section> 
            <hr>
            <section>
                <h3> My certificates</h3>
                <ul>
                     {{#each certificates}}
                    <li>
                        {{name}} -- {{id}}                           
                    </li>
                   {{/each}}
                </ul>                                       
            </section>
        </main>
    {{/with}}
</div>

